<h1>Contract\Contract</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Parent_id</th>
            <td>{{ entity.parentid }}</td>
        </tr>
        <tr>
            <th>Kod</th>
            <td>{{ entity.kod }}</td>
        </tr>
        <tr>
            <th>Serial_number</th>
            <td>{{ entity.serialnumber }}</td>
        </tr>
        <tr>
            <th>Title</th>
            <td>{{ entity.title }}</td>
        </tr>
        <tr>
            <th>Registered</th>
            <td>{{ entity.registered }}</td>
        </tr>
        <tr>
            <th>User_id</th>
            <td>{{ entity.userid }}</td>
        </tr>
        <tr>
            <th>Flat_id</th>
            <td>{{ entity.flatid }}</td>
        </tr>
        <tr>
            <th>Sale</th>
            <td>{{ entity.sale }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('contract') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('contract_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('contract_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
